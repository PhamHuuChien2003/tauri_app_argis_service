{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "goong-arcgis-service",
  "version": "0.1.0",
  "identifier": "com.goongpoiservice.tauriapp",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "goong-arcgis-service",
        "width": 100,
        "height": 60,
        "resizable": true,
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": false,
        "visible": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
